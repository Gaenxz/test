<?xml version="1.0"?>
<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:framework:Acl/etc/acl.xsd">
    <acl>
        <resources>
            <resource id="Magento_Backend::admin">
                <resource id="Magento_Backend::stores">

                    <resource id="Magento_Backend::stores_setting" title="store setting">
                        <resource id="Magenest_Movie::movie_index" title="Movie index"/>
                    </resource>

                    <resource id="Magento_Backend::stores_settings">
                        <resource id="Magento_Config::config">
                            <resource id="Magenest_Movie::config_movie" title="Movie Menu" >
                                <resource id="Magenest_Movie::section" title="Movie Section" >
                                    <resource id="Magenest_Movie::detail" title="Movie Detail" />
                                </resource>
                            </resource>
                            <resource id="Magenest_Movie::grid_data" title="Movie Grid Data" />
                        </resource>
                    </resource>

                </resource>
            </resource>
        </resources>
    </acl>
</config>